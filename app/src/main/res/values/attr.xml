<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--多重水波纹样式-->
    <declare-styleable name="MultiWaveHeader">
        <attr name="mwhWaveHeight" format="dimension"/>
        <attr name="mwhStartColor" format="color"/>
        <attr name="mwhCloseColor" format="color"/>
        <attr name="mwhAlphaColor" format="float"/>
        <attr name="mwhWaves" format="string"/>
    </declare-styleable>
    <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0" />
            <enum name="y" value="1" />
            <enum name="z" value="2" />
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="pivotX" format="fraction" />
        <attr name="pivotY" format="fraction" />
    </declare-styleable>
</resources>